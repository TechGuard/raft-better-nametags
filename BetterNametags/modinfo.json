{
  "name": "Better Nametags",
  "author": "TechGuard",
  "description": "Increases render distance for player nametags",
  "version": "1.1",
  "license": "GNU AGPLv3",
  "icon": "icon.png",
  "banner": "banner.png",
  "gameVersion": "1.0",
  "requiredByAllPlayers": false,
  "updateUrl": "https://www.raftmodding.com/api/v1/mods/better-nametags/version.txt",
  "isModPermanent": false,
  "excludedFiles": [],
  "modSettings": [
    {
      "type": "checkbox",
      "name": "NametagShowDistance",
      "text": "Nametag show distance",
      "access": "both",
      "default": true
    },
    {
      "type": "slider",
      "name": "NametagMaxDistance",
      "text": "Nametag maximum distance",
      "access": "both",
      "default": 250,
      "range": {
        "type": "number",
        "min": 15,
        "max": 250
      }
    },
    {
      "type": "slider",
      "name": "NametagSize",
      "text": "Nametag size",
      "access": "both",
      "default": 0.5,
      "range": {
        "type": "percent",
        "min": 0.1,
        "max": 2.0
      }
    }
  ]
}
